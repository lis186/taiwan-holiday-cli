{
  "name": "taiwan-holiday-cli",
  "version": "1.0.0",
  "description": "Taiwan holiday CLI tool - Query Taiwan holidays from command line",
  "type": "module",
  "bin": {
    "holiday": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "cli-table3": "^0.6.5",
    "ofetch": "^1.4.1",
    "conf": "^13.0.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "tsx": "^4.21.0",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "taiwan",
    "holiday",
    "calendar",
    "cli",
    "command-line"
  ],
  "author": "Justin Lee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lis186/taiwan-holiday-cli.git"
  },
  "bugs": {
    "url": "https://github.com/lis186/taiwan-holiday-cli/issues"
  },
  "homepage": "https://github.com/lis186/taiwan-holiday-cli#readme"
}
